class Solution {
    String firstAlphabet(String S) {
        
        String result=String.valueOf(S.charAt(0));
        
        int len=S.length();
        
        for(int i=0;i<len;i++)
        {
            if(S.charAt(i)==' ')
            {
                result=result+S.charAt(i+1);
            }
        }
        return result;
    }
};
