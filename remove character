class Solution{
    static String removeChars(String string1, String string2){
        
        
        String result=string1;
        int len=string2.length();
        
        
            for(int j=0;j<len;j++)
            {
               char replace=string2.charAt(j);
               result=result.replaceAll(String.valueOf(replace),"");
            }
        
        return result;
    }
}
